   
     {{#each article}}
       <div class="row">
        <div class="col s12">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">{{this.title}}</span>
            </div>
            <div class="card-action">
              <a href="{{this.link}}">learn more!</a>
              <button type="submit" class="saveBtn btn" value='{"title":"{{this.title}}","link":"{{this.link}}"}'>Save Article</button>
            </div>
          </div>
        </div>
      </div>
      {{/each}}
